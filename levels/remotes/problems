title = problems_title_306
cards = checkout add pull push commit-auto merge

[description]

problems_description_307

[setup yours]

echo "@@setup_auto_228@@" > file
git add .
git commit -m "initial"

git push -u friend main

echo "@@setup_auto_229@@" > file

[setup friend]

git checkout main

echo "@@setup_auto_230@@" > file
git commit -a -m "friends version"

[win]

# problems_win_hint_308
test "$(git status -s)" = ""

[win friend]

# problems_win_hint_309
git rev-parse main^ && test "$(git rev-parse main^1^)" = "$(git rev-parse main^2^)"

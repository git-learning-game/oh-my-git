title = steps_title_174
cards = checkout commit reset-hard add

[description]

steps_description_175

[setup]

echo @@setup_auto_057@@  > file1
echo @@setup_auto_058@@  > file2
git add .
git commit -m "Initial commit"

echo @@setup_auto_059@@  >> file1
echo @@setup_auto_060@@  >> file2

[win]

test "$(git diff-tree --no-commit-id --name-status -r main)" = "M	file2" &&
test "$(git diff-tree --no-commit-id --name-status -r main^)" = "M	file1"

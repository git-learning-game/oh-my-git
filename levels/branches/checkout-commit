title = checkoutcommit_title_085
cards = checkout commit-auto

[description]

checkoutcommit_description_086

[cli]

cli_checkoutcommit_015

cli_checkoutcommit_016

[setup]

echo "@@setup_auto_201@@" > piggy_bank
git add .
git commit -m "The beginning"

echo "@@setup_auto_202@@" > little_sister
git add .
git commit -m "Little sister comes in"

echo "@@setup_auto_203@@" >> little_sister
echo "@@setup_auto_204@@" > piggy_bank
git add .
git commit -m "Little sister does something"

git checkout HEAD^^
git branch -df main

[win]

# checkoutcommit_win_hint_088
{ git show HEAD:piggy_bank | grep "10 coins"; } && { test $(git show HEAD:little_sister | wc -l) -eq $(git show HEAD:little_sister | grep -v "10 coins" | wc -l); } && { git rev-parse HEAD^^^; }

[congrats]

checkoutcommit_congrats_087

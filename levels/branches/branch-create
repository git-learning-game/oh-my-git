title = branchcreate_title_089
cards = checkout commit-auto branch branch-delete reset-hard

[description]

branchcreate_description_090

[cli]

cli_branchcreate_009

[setup]

echo "@@setup_auto_193@@" > you
git add .
git commit -m "Evening preparations"
echo "@@setup_auto_194@@" >> you
git add .
git commit -m "Go to the birthday"

git checkout HEAD~1
echo "@@setup_auto_195@@" > you
git add .
git commit -m "Go to the concert"

git checkout HEAD~1

git branch -D main

[win]

# branchcreate_win_hint_092
git show birthday | grep 'to the birthday'

# branchcreate_win_hint_093
git show concert | grep 'to the concert'


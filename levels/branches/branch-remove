title = branchremove_title_108
cards = checkout commit-auto reset-hard branch-delete

[description]

branchremove_description_109

[setup]

echo @@setup_auto_186@@  > you
git add .
git commit -m "Good morning!"

echo @@setup_auto_187@@  >> you
git commit -am "Right side"

echo @@setup_auto_188@@  >> you
git commit -am "Jump"

git checkout HEAD^ -b friend
echo @@setup_auto_189@@  >> you
git commit -am "A new friend"

git checkout HEAD~2 -b music
echo @@setup_auto_190@@  >> you
git commit -am "Left side"

echo @@setup_auto_191@@  >> you
git commit -am "Sounds nice"

git checkout HEAD^ -b ice-cream
echo @@setup_auto_192@@  >> you
git commit -am "Yum"

git branch -M main leap
git checkout leap^^

[win]

# branchremove_win_hint_111
test "$(git show-ref --heads | cut -f2 -d' ')" = "$(echo refs/heads/leap)"

[congrats]

branchremove_congrats_110

[description]

checkout_description_074

[setup]

echo a > a
echo b > b
echo c > c
git add .
git commit -m "Initial commit"
echo x > a
echo x > b
echo x > c
git add c

[win]

# checkout_win_hint_075
test "$(git diff --name-only | wc -l)" -eq 0

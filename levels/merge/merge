title = merge_title_040
cards = checkout commit-auto merge

[description]

merge_description_041

[setup]

echo "@@setup_auto_001@@" > you

git add .
git commit -m "The Beginning"

echo "@@setup_auto_002@@" > you
git add .
git commit -m "You buy a baguette"

echo "@@setup_auto_003@@" > you
git add .
git commit -m "You eat the baguette"

git checkout HEAD~2
echo "@@setup_auto_004@@" > you
git add .
git commit -m "You buy some coffee"

echo "@@setup_auto_005@@" > you
git add .
git commit -m "You drink the coffee"

git checkout HEAD~2
echo "@@setup_auto_006@@" > you
git add .
git commit -m "You buy a donut"

echo "@@setup_auto_007@@" > you
git add .
git commit -m "You eat the donut"

git checkout --detach
git branch -D main

[win]

# merge_win_hint_043
{ git show HEAD:you | grep "You ate.*baguette"; } && { git show HEAD:you | grep "You drank.*coffee"; } && { git show HEAD:you | grep "You ate.*donut"; }

# merge_win_hint_044
test "$(git log --pretty=%P -n 1 HEAD | wc -w)" -ge 2

[congrats]

merge_congrats_042

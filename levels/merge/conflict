title = conflict_title_045
cards = checkout commit-auto merge reset-hard

[description]

conflict_description_046

[setup]

echo "@@setup_auto_011@@" > sam
git add .
git commit -m "The beginning"

git checkout -b pancakes
echo "@@setup_auto_012@@" > sam
git add .
git commit -m "Pancakes!"

echo "@@setup_auto_013@@" >> sam
git commit -am "Go to work"

git checkout -b muesli main
echo "@@setup_auto_014@@" > sam
git add .
git commit -m "Muesli!"

echo "@@setup_auto_015@@" >> sam
git commit -am "Go to work"

git checkout main

[win]

# conflict_win_hint_048
git rev-parse main^ && test "$(git rev-parse main^1^^)" = "$(git rev-parse main^2^^)"

[congrats]

conflict_congrats_047

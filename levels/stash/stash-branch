[setup]

echo "@@setup_auto_174@@" > recipe

git add .
git commit -m "creating a recipe"

echo "@@setup_auto_175@@" >> recipe

git add .
git commit -m "Adding ingredients"

echo "@@setup_auto_176@@" >> recipe
git stash push

git checkout main


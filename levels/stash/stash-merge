title = stashmerge_title_156
cards = checkout commit-auto merge reset-hard

[description]

stashmerge_description_157

[setup]

echo "@@setup_auto_167@@" > recipe

git add .
git commit -m "creating a recipe"

echo "@@setup_auto_168@@" >> recipe

git add .
git commit -m "Adding ingredients"

echo "@@setup_auto_169@@" >> recipe

git stash push

echo "@@setup_auto_170@@" >> recipe

git checkout main
git add recipe

[win]

# stashmerge_win_hint_158
{ git show HEAD | grep "Flour"; } && { git show HEAD | grep "Salt"; }

# stashmerge_win_hint_159
test "$(git stash list | wc -l)" -eq 0

[actions]



[congrats]

stashpop_congrats_150

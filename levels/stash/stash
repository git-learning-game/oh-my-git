[setup]

echo "@@setup_auto_171@@" > recipe

git add .
git commit -m "creating a recipe"

echo "@@setup_auto_172@@" >> recipe

git add .
git commit -m "Adding ingredients"

echo "@@setup_auto_173@@" >> recipe

git checkout main

[win]

# stash_win_hint_147
test "$(git stash list | wc -l)" -ge 1

[actions]




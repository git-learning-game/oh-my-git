title = stashpop_title_148
cards = checkout commit-auto merge reset-hard

[description]

stashpop_description_149

[setup]

echo "@@setup_auto_164@@" > recipe

git add .
git commit -m "creating a recipe"

echo "@@setup_auto_165@@" >> recipe

git add .
git commit -m "Adding ingredients"

echo "@@setup_auto_166@@" >> recipe

git stash push
git checkout main

[win]

# stashpop_win_hint_151
test "$(git stash list | wc -l)" -eq 0

[actions]



[congrats]

stashpop_congrats_150

title = stashclear_title_152
cards = checkout commit-auto merge reset-hard

[description]

stashclear_description_153

[setup]

echo "@@setup_auto_159@@" > recipe

git add .
git commit -m "creating a recipe"

echo "@@setup_auto_160@@" >> recipe

git add .
git commit -m "Adding ingredients"

echo "@@setup_auto_161@@" >> recipe
git stash push

echo "@@setup_auto_162@@" >> recipe
git stash push

echo "@@setup_auto_163@@" >> recipe
git stash push

git checkout main

[win]

# stashclear_win_hint_155
test "$(git stash list | wc -l)" -eq 0

[actions]



[congrats]

stashclear_congrats_154
